@{
    ViewData["Title"] = "Mes cessions de pêche";
}
    <a>
        @Html.ActionLink("Ajouter une nouvelle cession","Create","Cession")
    </a>
<table class="table table-striped">
    <tr>
        <th>ID</th>
        <th>Date</th>
        <th>Site</th>
        <th>Marée</th>
        <th>Météo</th>
        <th>Action</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.id</td>
            <td>@item.date</td>
            <td>@item.site_nom</td>
            <td>@item.maree</td>
            <td>@item.meteo</td>
            <td>
                <a>
                    @Html.ActionLink("Consulter","Detail",new {id = item.id})
                </a>
            </td>
        </tr>
    }
</table>
